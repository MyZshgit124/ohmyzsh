# common functions
alias d-a='sudo /DietPi/dietpi/dietpi-autostart'
alias d-b='sudo /DietPi/dietpi/dietpi-backup'
alias d-ba='/DietPi/dietpi/func/dietpi-banner'
alias d-c='sudo /DietPi/dietpi/dietpi-config'
alias d-cl='sudo /DietPi/dietpi/func/dietpi-logclear'
alias d-cs='sudo /DietPi/dietpi/dietpi-cloudshell'
alias d-d='sudo /DietPi/dietpi/dietpi-drive_manager'
alias d-ds='sudo /Dietpi/dietpi/func/set_dphys-swapfile'
alias d-e='sudo /DietPi/dietpi/dietpi-explorer'
alias d-g='sudo /Dietpi/dietpi/func/set_cpu'
alias d-h='sudo /DietPi/dietpi/dietpi-cpuinfo'
alias d-help='cat ~/.oh-my-zsh/plugins/dietpi/README.md'
alias d-i='sudo /DietPi/dietpi/dietpi-software'
alias d-l='sudo /DietPi/dietpi/dietpi-launcher'
alias d-lc='sudo /DietPi/dietpi/dietpi-letsencrypt'
alias d-ld='sudo /DietPi/dietpi/func/dietpi-led_control'
