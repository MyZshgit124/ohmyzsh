alias d-p='sudo /DietPi/dietpi/dietpi-process_tool'
alias d-r='sudo /DietPi/dietpi/dietpi-sync'
alias d-s='sudo /DietPi/dietpi/dietpi-services'
alias d-sac='sudo /DietPi/dietpi/dietpi-services systemd_controlled && sudo /DietPi/dietpi/dietpi-services dietpi_controlled'
alias d-sad='sudo /DietPi/dietpi/dietpi-services disable all'
alias d-sae='sudo /DietPi/dietpi/dietpi-services enable all'
alias d-ss='sudo /DietPi/dietpi/dietpi-services status'
alias d-sr='sudo /DietPi/dietpi/dietpi-services restart'
alias d-su='sudo /DietPi/dietpi/dietpi-services unmask'
alias d-sm='sudo /DietPi/dietpi/dietpi-services mask'
alias d-t='sudo /DietPi/dietpi/dietpi-cleaner'
alias d-u='sudo /DietPi/dietpi/dietpi-update'

